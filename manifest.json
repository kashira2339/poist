{
    "name": "Poist",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Poist create post-it every domain.",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["js/poist.js", "js/poist_controller.js", "js/poist_holder.js", "js/main.js"],
            "css": ["css/main.css"]
        }
    ]
}
