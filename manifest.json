{
    "name": "Poist",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Poist create post-it every domain.",
    "permissions": [
        "contextMenus"
    ],
    "icons": {
        "16": "icon/16.png",
        "48": "icon/48.png",
        "128":"icon/128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["js/poist.js", "js/poist_controller.js", "js/poist_holder.js", "js/main.js"],
            "css": ["css/main.css"]
        }
    ]
}
